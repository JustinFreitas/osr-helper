  "description": "A collection of helpers for the Foundry Vtt implementation of the 'Old School Essentials' system.",
  "authors": [
    {
      "name": "Rabid Owlbear",
      "url": "https://github.com/RabidOwlbear"
    }
  ],
  "version": "0.3.29",
  "minimumCoreVersion": "0.8.6",
  "compatibleCoreVersion": "10",
  "compatibility": {
    "minimum": "10",
    "verified": "10.287",
    "maximum": "10"
  },
  "styles": ["./styles/main.css"],
  "esmodules": ["./scripts/init.js"],
