      "path": "packs/items.db",
      "entity": "Item",
      "module": "osr-helper",
      "system": "ose"
    },
    {
      "name": "osr-helper-patch-notes",
      "label": "OSR-helper Patch Notes",
      "path": "packs/patch-notes.db",
      "type": "JournalEntry",
      "module": "osr-helper",
      "system": "ose"
    },
    {
      "name": "osr-helper-macros",
      "label": "OSR-helper Macros",
      "path": "packs/macros.db",
      "type": "Macro",
      "module": "osr-helper",
      "system": "ose"
    }
  ],
  "relationships": {
    "systems": [{ "id": "ose" }, { "id": "ose-dev" }],
    "requires": [
      {
        "id": "compendium-folders",
